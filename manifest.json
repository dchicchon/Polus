{
    "name": "Planner",
    "version": "1.0.0.1",
    "manifest_version": 2,
    "description": "An online planner to help make you more effecient!",
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "alarms"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}